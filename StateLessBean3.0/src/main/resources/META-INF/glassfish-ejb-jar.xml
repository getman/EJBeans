<!DOCTYPE glassfish-ejb-jar PUBLIC "-//GlassFish.org//
DTD GlassFish Application Server 3.1 EJB 3.1//EN"
                "http://glassfish.org/dtds/glassfish-ejb-jar_3_1-1.dtd">
<glassfish-ejb-jar>
    <security-role-mapping>
        <role-name>admin</role-name>
        <principal-name>getman</principal-name>
        <group-name>asadmin</group-name>
    </security-role-mapping>
    <disable-nonportable-jndi-names>true</disable-nonportable-jndi-names>
</glassfish-ejb-jar>